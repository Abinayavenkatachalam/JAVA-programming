        int m=sc.nextInt();
        System.out.println("Bird said:"+m);
        
        int x=m/10;
        int y=m%10;
        System.out.println("Alice must go in path-"+(x+y));
        
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
