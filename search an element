import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;


public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    Scanner sc = new Scanner (System.in);
        int m = sc.nextInt();
        int flag=0;
        int arr[]=new int[m];
        for(int i=0;i<m;i++){
            arr[i]=sc.nextInt();
            
        }
        int tar=sc.nextInt();
        for(int i=0;i<m;i++){
            if(arr[i]==tar){
                flag++;
            }
            
        }
        if(flag==1){
            System.out.println(tar+" is presented in an array.");
        }
        else{
            System.out.println(tar+" is not presented in an array.");
        }
    }
}
       
