import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a= s.nextInt();
        int b= s.nextInt();
        int c= s.nextInt();
        int m = ((b-c)*a)-100;
        
    System.out.println("Number of copies sold:"+a);
        System.out.println("Cost of each copy:"+b);
        System.out.println("Cost spent by agency on each newspaper:"+c);
        System.out.printf("The profit obtained is Rs.%.2f",(double)m);
        
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
