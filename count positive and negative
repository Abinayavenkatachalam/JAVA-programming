import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
         Scanner sc = new Scanner (System.in);
        int m = sc.nextInt();
        int flag=0,count=0;
        int a[]=new int[m];
        for(int i=0;i<m;i++){
            a[i]=sc.nextInt();
            
        }
        for(int i=0;i<m;i++){
            if(a[i]<=0){
                flag++;
            }
            else{
                count++;
            }
        
    }
        System.out.printf("Count of Positive Integer is %.2f\n",(float)count);
         System.out.printf("Count of Negative Integer is %.2f",(float)flag);
    }
}
